<!-- *** Bundle  -->

<item name="qt_teleport_to_trader_mplogue_bundle">
	<property name="DescriptionKey" value="qt_teleport_to_trader_mplogue_bundle_desc"/>
	<property name="CustomIcon" value="helipad_bright"/>
	<!-- <property name="CustomIconTint" value="66ccff" /> -->
	<property name="CustomIconTint" value="ffff00" />
	<!-- <property name="ItemTypeIcon" value="treasure"/> -->
