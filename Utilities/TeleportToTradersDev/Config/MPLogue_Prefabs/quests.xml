<!-- trader_rekt_by_MPLogue  -->
<!-- cntServiceTruckWorkingStiffTools -->
<quest id="q_goToPOI_trader_rekt_mplogue">
    <property name="group_name_key" value="q_goToPOI_teleport_to_traders"/>
    <property name="name_key" value="q_goToPOI_trader_rekt"/>
    <property name="subtitle_key" value="q_goToPOI_trader_rekt_subtitle"/>
    <property name="description_key" value="q_goToPOI_trader_rekt_description"/>
    <property name="icon" value="ui_game_symbol_book"/>
    <property name="category_key" value="quest"/>
    <property name="offer_key" value="q_goToPOI_trader_rekt_offer"/>
    <property name="difficulty" value="veryeasy"/>
    <property name="shareable" value="false"/>
    <property name="add_to_tier_complete" value="false"/>
    <property name="repeatable" value="false"/>
 
	<action type="TrackQuest"/>
	<action type="GiveBuffSDX, SCore">
		<property name="value" value="buff_fadetoblack" />
		<property name="phase" value="1" />
		<property name="delay" value="1"/>				
	</action>	
 
	<objective type="GotoPOISDX, SCore" value="100-16000" phase="1">
		<property name="completion_distance" value="50"/>
		<property name="PrefabName" value="trader_rekt_by_MPLogue"/>
	</objective>	

	<action type="Teleport, SCore">
		<property name="phase" value="1" />
		<property name="delay" value="5" />
		<property name="offset" value="0,0,0" />
	</action>	

	<action type="Teleport, SCore">
		<property name="block" value="cntServiceTruckWorkingStiffTools" />
		<property name="phase" value="1" />
		<property name="delay" value="6" />
		<property name="offset" value="0,0,0" />
	</action>

	<action type="GiveBuffSDX, SCore">
		<property name="value" value="buff_fadetowhite" />
		<property name="on_complete" value="true"/>
		<property name="phase" value="1" />
		<property name="delay" value="1"/>				
	</action>		

    <reward type="Exp" value="1"/>
</quest>

