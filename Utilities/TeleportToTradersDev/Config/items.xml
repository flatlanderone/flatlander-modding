<!-- *** Navezgane 2.4 bundle for vanilla traders.  -->

<item name="qt_teleport_to_trader_navezgane_24_bundle">
	<property name="DescriptionKey" value="qt_teleport_to_trader_navezgane_24_bundle_desc"/>
	<property name="CustomIcon" value="flagPoleTrader_bright"/>
	<!-- <property name="CustomIconTint" value="ff7400" /> -->
	<property name="CustomIconTint" value="66ccff" />
	<!-- <property name="ItemTypeIcon" value="treasure"/> -->
	<property name="ItemTypeIcon" value="map_trader"/>
	<property name="HoldType" value="45"/>
	<property name="Meshfile" value="@:Other/Items/Misc/sackPrefab.prefab"/>
	<property name="DropMeshfile" value="@:Other/Items/Misc/sack_droppedPrefab.prefab"/>
	
	<property name="Material" value="Morganic"/>
	<property name="Stacknumber" value="5"/>
	<property name="EconomicValue" value="6000"/>
	<property name="SellableToTrader" value="false"/>
	<property name="Group" value="Resources"/>
	<property name="SoundPickup" value="stone_grab"/>
	<property name="SoundPlace" value="stone_place"/>
	<property class="Action0">
		<property name="Class" value="OpenBundle"/>
		<property name="Delay" value="0"/>
		<property name="Sound_start" value="close_garbage"/>
		<property name="Create_item" value="qt_teleport_to_trader_rekt_navezgane_24,qt_teleport_to_trader_jen_navezgane_24,qt_teleport_to_trader_bob_navezgane_24,qt_teleport_to_trader_hugh_navezgane_24,qt_teleport_to_trader_joel_navezgane_24"/>
